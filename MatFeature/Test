#ifndef TEST_H
#define TEST_H

#include <iostream>
#include <vector>
using namespace std;

#define dbg(x) cout << #x << " : " << x << endl

template<class T>
void debug(vector<T> v) {
    for (T t : v) {
        cout << t << ' ';
    }
    cout << endl;
}

template<class T>
void debug(vector<vector<T>> v) {
    for (vector<T> t : v) {
        debug(t);
    }
}

#endif